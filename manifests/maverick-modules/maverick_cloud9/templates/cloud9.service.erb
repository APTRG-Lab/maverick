[Unit]
Description=Cloud9
After=network-online.target
Requires=network-online.target

[Service]
TimeoutStartSec=0
ExecStart=/srv/maverick/.c9/node/bin/node /srv/maverick/software/cloud9/server.js -p <%= @webport %> -l 0.0.0.0 -w <%= @basepath %> --packed --cache -a mav:wingman
User=mav

[Install]
WantedBy=multi-user.target

