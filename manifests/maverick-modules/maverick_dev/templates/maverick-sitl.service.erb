[Unit]
Description=Maverick ArduPilot SITL
After=network-online.target
Requires=network-online.target

[Service]
TimeoutStartSec=0
Type=forking
EnvironmentFile=/srv/maverick/data/config/mavlink/sitl.conf
ExecStart=/srv/maverick/software/maverick/bin/sitl.sh
User=mav

[Install]
WantedBy=multi-user.target

