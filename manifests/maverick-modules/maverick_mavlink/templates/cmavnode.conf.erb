<% if @udpinaddress and @udpinport %>
[linkname]
    type=socket
    targetip=<%= @udpinaddress %>
    targetport=<%= @udpinport %>
<% else -%>
[fcserial]
    type=serial
    port=<%= @inputaddress %>
    baud=115200
    flow_control=false
<% end -%>

<% (0..(@udpports.to_i()-1)).each do |i| -%>
[udpin<%= i -%>]
    type=socket
    localport=<%= @startingudp.to_i() + i %>
<% end -%>
