[Unit]
Description=Maverick Mavros - FC
After=network-online.target
Requires=network-online.target

[Service]
TimeoutStartSec=0
ExecStart=/bin/bash -a -c 'source /opt/ros/<%= @distribution %>/setup.bash && ROS_NAMESPACE="mavfc" /opt/ros/<%= @distribution %>/bin/roslaunch mavros apm.launch fcu_url:=tcp://localhost:5765'
User=mav

[Install]
WantedBy=multi-user.target

