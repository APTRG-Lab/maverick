[General]
report-stats=false
tcp-server-port=<%= @startingtcp %>

<% if @inputtype == "tcp" -%>
[TcpEndpoint tcpin]
    Address=<%= @inputaddress %>
    Port=<%= @inputport %>
    RetryTimeout=5
<% elsif @inputtype == "serial" %>
[UartEndpoint serialin]
    Device=<%= @inputaddress %>
<% end -%>

<% (0..(@udpports.to_i()-1)).each do |i| -%>
[UdpEndpoint <%= i -%>]
    Mode = Eavesdropping
    Address = 0.0.0.0
    Port = <%= @startingudp.to_i() + i %>
<% end %>
